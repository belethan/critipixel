{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,

  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-pgsql": "*",
    "doctrine/dbal": "^3.10.3",
    "doctrine/doctrine-bundle": "^2.18.1",
    "doctrine/doctrine-migrations-bundle": "^3.6.0",
    "doctrine/orm": "^3.1",
    "phpdocumentor/reflection-docblock": "^5.6.3",
    "phpstan/phpdoc-parser": "^1.28",
    "stof/doctrine-extensions-bundle": "^1.14.0",
    "symfony/asset": "^7.3",
    "symfony/asset-mapper": "^7.3",
    "symfony/flex": "^2",
    "symfony/form": "7.3.6.*",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/property-info": "^7.3",
    "symfony/runtime": "^7.3",
    "symfony/security-csrf": "7.3.*",
    "symfony/serializer": "^7.3",
    "symfony/string": "^7.3",
    "symfony/translation": "^7.3",
    "symfony/translation-contracts": "^3.6.1",
    "symfony/ux-twig-component": "^2.31.0",
    "symfony/validator": "7.3.*",
    "symfony/web-link": "^7.3",
    "symfony/yaml": "^7.3",
    "symfonycasts/sass-bundle": "^0.7",
    "twbs/bootstrap": "^5.3.8",
    "twig/extra-bundle": "^3.22",
    "twig/string-extra": "^3.22",
    "twig/twig": "^3.22",
    "vich/uploader-bundle": "^2.8"
  },

  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },

  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "config/helpers.php"
    ]
  },

  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },

  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },

    "scripts": {
        "db": [
            "symfony console doctrine:database:drop -f --if-exists",
            "symfony console doctrine:database:create",
            "symfony console doctrine:migrations:migrate -n",
            "symfony console doctrine:fixtures:load -n --purge-with-truncate"
        ],
        "db-test": [
            "symfony console doctrine:database:drop -f --if-exists --env=test",
            "symfony console doctrine:database:create --env=test",
            "symfony console doctrine:migrations:migrate -n --env=test",
            "symfony console doctrine:fixtures:load -n --purge-with-truncate --env=test"
        ],
        "test": [
            "composer db-test",
            "symfony php bin/phpunit"
        ],
        "analyse": "vendor/bin/phpstan analyse --memory-limit=1G",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },

  "conflict": {},

  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*"
    }
  },

  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "fakerphp/faker": "^1.23",
    "friendsofphp/php-cs-fixer": "^3.91",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^10.5",
    "symfony/asset": "^7.3",
    "symfony/browser-kit": "^7.3",
    "symfony/console": "^7.3",
    "symfony/css-selector": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/maker-bundle": "^1.58",
    "symfony/monolog-bundle": "^3.0",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/routing": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/stopwatch": "^7.3",
    "symfony/string": "^7.3",
    "symfony/translation": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/web-profiler-bundle": "^7.3",
    "symfony/yaml": "^7.3"
  }
}
